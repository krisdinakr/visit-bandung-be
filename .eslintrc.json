{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "import/no-dynamic-require": 0,
      "global-require": 0
    }
}
